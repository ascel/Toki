#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define INF (1e9 + 5)
#define fi first
#define se second

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);
    int max_weight, n;
    cin >> max_weight >> n;
    vector<vector<int>> store(max_weight + 1, vector<int> ());
    vector<int> dp(max_weight + 1, INF);
    dp[0] = 0;
    for(int i = 0; i < n; i++) {
        int weight, val;
        cin >> weight >> val;
        for(int weight_already = max_weight - weight; weight_already >= 0; weight_already--) {
            if(dp[weight_already] + val < dp[weight_already + weight]) {
                dp[weight_already + weight] = dp[weight_already] + val; 
                store[weight_already + weight] = store[weight_already];
                store[weight_already + weight].push_back(i + 1);
            }
        }
    }
    int ans = 0;
    int id = 0;
    for(int i = 0; i <= max_weight; i++) {
        if(dp[i] != INF) {
            if(ans < dp[i]) {
                ans = dp[i];
                id = i;
            }
        }
    }
    for(int i = 0; i < store[id].size(); i++) {
        cout << store[id][i] << endl;
    }
    return 0;
}
